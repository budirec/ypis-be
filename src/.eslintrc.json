{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "commonJS"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": ["**/dist/*"],
    "rules": {
        "indent": ["error", 2]
    }
}
